{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "none",
    "rootDir": "./src",
    "outDir": "./build",
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "sourceMap": false
  },
  "include": ["src/**/*.ts"]
}
