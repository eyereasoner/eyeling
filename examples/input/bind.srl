PREFIX : <http://example/#>

DATA { :phayes a :Person ; :givenName "Pat"; :familyName "Hayes" . }

# Default value - calculate a name
RULE { ?x :name ?FN } WHERE { 
    ?x a :Person 
    NOT { ?x :name ?someName }
    ?x :givenName ?name1 ;
       :familyName ?name2 .
    BIND(concat(?name1, " ", ?name2) AS ?FN)
}
