{
  "name": "eyeling",
  "version": "1.9.2",
  "description": "A minimal Notation3 (N3) reasoner in JavaScript.",
  "main": "./index.js",
  "keywords": [
    "N3",
    "reasoner"
  ],
  "license": "MIT",
  "author": "Jos De Roo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eyereasoner/eyeling.git"
  },
  "homepage": "https://github.com/eyereasoner/eyeling#readme",
  "bin": {
    "eyeling": "./eyeling.js"
  },
  "types": "./index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "eyeling.js",
    "eyeling-builtins.ttl",
    "src",
    "tsconfig.json",
    "test",
    "examples",
    "README.md",
    "LICENSE.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test:packlist": "node test/packlist.test.js",
    "test:api": "node test/api.test.js",
    "test:maken3": "node test/maken3.test.js",
    "test:examples": "node test/examples.test.js",
    "test:package": "node test/package.test.js",
    "test": "npm run test:packlist && npm run test:api && npm run test:maken3 && npm run test:examples",
    "preversion": "npm test",
    "postversion": "git push --follow-tags",
    "build": "node scripts/build.mjs",
    "pretest": "npm run build",
    "pretest:packlist": "npm run build",
    "pretest:api": "npm run build",
    "pretest:maken3": "npm run build",
    "pretest:examples": "npm run build",
    "pretest:package": "npm run build",
    "prepack": "npm run build -- --quiet"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
