{
  "name": "eyeling",
  "version": "1.11.7",
  "description": "A minimal Notation3 (N3) reasoner in JavaScript.",
  "main": "./index.js",
  "keywords": [
    "N3",
    "reasoner"
  ],
  "license": "MIT",
  "author": "Jos De Roo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eyereasoner/eyeling.git"
  },
  "homepage": "https://github.com/eyereasoner/eyeling#readme",
  "bin": {
    "eyeling": "./eyeling.js"
  },
  "types": "./index.d.ts",
  "files": [
    "LICENSE.md",
    "README.md",
    "HANDBOOK.md",
    "examples",
    "eyeling-builtins.ttl",
    "eyeling.js",
    "index.d.ts",
    "index.js",
    "lib",
    "test",
    "tools"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "node tools/bundle.js",
    "test:packlist": "node test/packlist.test.js",
    "test:api": "node test/api.test.js",
    "test:n3gen": "node test/n3gen.test.js",
    "test:examples": "node test/examples.test.js",
    "test:manifest": "node test/manifest.test.js",
    "test:playground": "node test/playground.test.js",
    "test:package": "node test/package.test.js",
    "test:all": "node tools/test.js",
    "pretest": "npm run -s lint && npm run -s build",
    "test": "npm run -s test:all",
    "preversion": "npm test",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "globals": "^17.1.0"
  }
}
